{
  "name": "@marchyang/init-eslint",
  "version": "1.1.7",
  "description": "",
  "main": "./lib/index.js",
  "bin": "./lib/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc && rollup -c && cp ./.eslintrc.js ./lib/",
    "ts:run": "TS_NODE_PROJECT=./tsconfig_debugger/tsconfig.json node --loader ts-node/esm ./src/index.ts",
    "ts:debugger": "TS_NODE_PROJECT=./tsconfig_debugger/tsconfig.json node --inspect-brk  --loader ts-node/esm  ./src/index.ts",
    "prepublish": "npm run build && npm run removeType",
    "removeType": "TS_NODE_PROJECT=./tsconfig_debugger/tsconfig.json node --loader ts-node/esm removeType.ts"
  },
  "files": [
    "lib/*",
    "*.md",
    "package.json"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-image": "^2.1.1",
    "@types/node": "^16.11.11",
    "@types/react": "^17.0.24",
    "less": "^4.1.1",
    "postcss": "^8.3.7",
    "rollup": "^2.57.0",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-typescript": "^1.0.1",
    "ts-node": "^10.4.0",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@marchyang/execa": "^6.0.1",
    "@rollup/plugin-commonjs": "^21.0.1",
    "listr2": "^3.13.5",
    "react": "^17.0.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-externals": "^2.2.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}